{
    "configurations": [
      {
        "type": "gdb",
        "request": "attach",
        "name": "(gdb) Debug Mod",
        "target": "127.0.0.1:3333",
        "remote": true,
        "stopAtConnect": true,
        "executable": "debug/mod.elf",
        "gdbpath": "/usr/bin/gdb-multiarch",
        "windows": {
          "gdbpath": "gdb-multiarch"
        },
        "cwd": "${workspaceRoot}",
        "autorun": [
          "set confirm off",
          "set substitute-path /project ."
        ],
        "valuesFormatting": "parseText"
      }
    ]
}